<%= link_to('<< Back to list',{:action=>'index'},:class=>'Back link') %>

<div class="Page show">
  <h2>Show Page</h2>

  <table summary="Page detail view">
    <tr>
      <th>Name</th>
      <td><%= @page.name %></td>
    </tr>
    <tr>
      <th>Position</th>
      <td><%= @page.position %></td>
    </tr>
    <tr>
      <th>Visible</th>
      <td><%= @page.visible%></td>
    </tr>
    <tr>
      <th>Permalink</th>
      <td><%= @page.permalink %></td>
    </tr>
    <tr>
      <th>Created at</th>
      <td><%= @page.created_at %></td>
    </tr>
    <tr>
      <th>Updated at</th>
      <td><%= @page.updated_at%></td>
    </tr>

  </table>
</div>
